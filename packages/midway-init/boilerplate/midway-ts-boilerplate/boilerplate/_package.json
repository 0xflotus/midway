{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "private": true,
  "dependencies": {
    "midway": "*",
    "tslib": "^1.8.1",
    "typescript": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.5",
    "egg-ci": "^1.8.0",
    "midway-bin": "*",
    "midway-mock": "*",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-{{name}}",
    "stop": "egg-scripts stop --title=egg-server-{{name}}",
    "dev": "NODE_ENV=local midway-bin dev --ts",
    "debug": "NODE_ENV=local midway-bin debug --ts",
    "test": "npm run lint -- --fix && midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint -c tslint.json --project .",
    "ci": "midway-bin cov --ts",
    "build": "midway-bin build -c"
  },
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "{{author}}",
  "license": "MIT"
}
