{
  "name": "midway",
  "version": "0.3.3",
  "description": "Midway Main Package",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run lint && midway-bin build -c",
    "lint": "../../node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts test/**/*.ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "ci": "npm run test"
  },
  "keywords": [
    "midway",
    "IoC",
    "webframework"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^10.5.5",
    "midway-bin": "^0.3.2",
    "mm": "^2.2.0",
    "mocha": "^3.5.0",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "egg-cluster": "^1.17.0",
    "injection": "^0.3.2",
    "midway-core": "^0.3.3",
    "midway-web": "^0.3.3"
  },
  "files": [
    "bin",
    "cluster",
    "dist",
    "server.js"
  ],
  "engines": {
    "node": ">= 8.0.0"
  }
}
